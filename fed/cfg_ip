#/bin/ksh

host=`/bin/hostname | awk -F\. '{print $1}'`

#Cec 1
if [ "$host" = "c462f30rp01" ]
then
    echo " Configuring sn0,sn1,ml0 for $host "
    ifconfig sn0 inet 134.9.65.1 netmask 255.255.255.0
    ifconfig sn1 inet 134.9.66.1 netmask 255.255.255.0
    ifconfig ml0 inet 134.9.64.1 netmask 255.255.255.0
fi
